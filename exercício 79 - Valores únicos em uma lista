 num = list()

while True:
    valor = (int(input('Digite um valor: ')))

    if valor not in num:
        num.append(valor)
        print('Valor adicionado com sucesso!')
    else:
        print('Valor duplicado! Não vou adicionar...')

    res = str(input('Quer continuar? [S/N] ')).upper().strip()[0]
    while res not in 'SN':
        res = str(input('Quer continuar? [S/N] ')).upper().strip()[0]

    if res == 'N':
        break

print(f'{"-=-" * 20}')
num.sort()
print(f'Você digitou os valores {num}')
